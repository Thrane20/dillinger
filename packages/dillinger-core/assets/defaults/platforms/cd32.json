{
  "$schema": "../../schema/platform-v1.0.json",
  "schemaVersion": "1.0",
  "id": "cd32",
  "name": "Amiga CD32",
  "type": "emulator",
  "description": "Commodore Amiga CD32 via FS-UAE emulator.",
  "configuration": {
    "containerImage": "ghcr.io/thrane20/dillinger/runner-fs-uae",
    "supportedExtensions": [".adf", ".ipf", ".dms", ".lha", ".lzh", ".lzx", ".zip", ".rp9", ".info", ".hdf", ".iso", ".cue"],
    "defaultSettings": {
      "environment": {
        "DISPLAY": ":0",
        "FSUAE_CONFIG_DIR": "/config/fs-uae",
        "FSUAE_KICKSTARTS_DIR": "/bios"
      },
      "launch": {
        "command": "fs-uae",
        "arguments": ["--amiga-model=CD32"]
      }
    }
  },
  "validation": {
    "fileValidation": [".adf", ".ipf", ".dms", ".lha", ".lzh", ".lzx", ".zip", ".rp9", ".info", ".hdf", ".iso", ".cue"]
  },
  "displayStreaming": {
    "method": "x11"
  },
  "isActive": true,
  "created": "2025-11-24T12:00:00Z",
  "updated": "2025-11-24T12:00:00Z"
}
